<div id="corpo">
  <h1>SGTRAN</h1>
  <% form_for(@usuario_session) do |f| %>
    <%= f.error_messages %>

    <p>
      <%= f.label :login, "Login:" %>
      <%= f.text_field :login, :class => "text" %>
    </p>
    <p>
      <%= f.label :password, "Senha:" %>
      <%= f.password_field :password, :class => "text" %>
    </p>
    <p>
      <%= f.submit 'Entrar', :class => "submit" %>
    </p>
  <% end %>
</div>

