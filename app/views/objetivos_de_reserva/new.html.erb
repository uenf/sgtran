<div class="show_padrao">

<h1>Novo Objetivo de reserva</h1>

<% form_for(@objetivo_de_reserva) do |f| %>
  <%= mensagem_de_erro(@objetivo_de_reserva) %>

  <div class="show_linha">
    <div class="linha_titulo"><%= f.label :texto, "Texto:", :class => "label_obrigatorio" %></div>
    <div class="linha_texto_neutro"><%= f.text_field :texto %></div>
  </div>

  <div class="botao_concluir"><%= f.submit 'Criar Objetivo de reserva' %></div>

<% end %>
</div>

<div id="opcoes_da_visao">
  <ul>
    <li id="opcao_voltar" onclick="history.back(-1)" onmouseover="this.style.cursor='pointer'">Voltar</li>
  </ul>
</div>

