<div class="show_padrao">

<h1>Dados do Solicitante</h1>
  <div class="show_linha">
    <div class="linha_titulo">Nome:</div>
    <div class="linha_texto"><%=h @solicitante.nome %></div>
  </div>

  <div class="show_linha">
    <div class="linha_titulo">Email:</div>
    <div class="linha_texto"><%=h @solicitante.email %></div>
  </div>

  <div class="show_linha">
    <div class="linha_titulo">Matricula:</div>
    <div class="linha_texto"><%=h @solicitante.matricula %></div>
  </div>

  <div class="show_linha">
    <div class="linha_titulo">Cargo:</div>
    <div class="linha_texto"><%=h @solicitante.cargo %></div>
  </div>

</div>
<div id="opcoes_da_visao">
  <ul>
    <li id="opcao_voltar" onclick="location.href='<%= solicitantes_path %>'" onmouseover="this.style.cursor = 'pointer'"><%= link_to 'Voltar', solicitantes_path %></li>
  </ul>
</div>

